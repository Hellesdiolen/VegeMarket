<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="com.portfolio.biz.member.MemberVO" alias="member"/>
		<typeAlias type="com.portfolio.biz.member.AddressVO" alias="address"/>
		<typeAlias type="com.portfolio.biz.order.CartVO" alias="cart"/>
